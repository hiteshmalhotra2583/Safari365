    <validationRules>
        <fullName>Referral_Client_Name</fullName>
        <active>true</active>
        <description>When a lead is a Client Referral, the referring clients name must be provided.</description>
        <errorConditionFormula>AND
(ISPICKVAL(  LeadSource , &quot;Client Referral&quot;),ISBLANK( Referral_Client_Name__c ))</errorConditionFormula>
        <errorDisplayField>Referral_Client_Name__c</errorDisplayField>
        <errorMessage>Please provide the clients name as per Salesforce that referred this client (a grift will be sent to him/her should booking occur)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Unqualified_Lead</fullName>
        <active>true</active>
        <description>When a lead status is Unqualified, a reason must be given.</description>
        <errorConditionFormula>AND
(ISPICKVAL( Status  , &quot;Unqualified&quot;),ISBLANK( TEXT(Unqualified_Reason__c)  ))</errorConditionFormula>
        <errorDisplayField>Absolute_luxury__c</errorDisplayField>
        <errorMessage>Please provide an Unqualified reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_country_on_convert</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT($User.ByPassValidation__c),
LEN(TEXT( New_Country_of_Residence__c ) )= 0,
 IsConverted 
)</errorConditionFormula>
        <errorDisplayField>New_Country_of_Residence__c</errorDisplayField>
        <errorMessage>Please ensure a Country value is included, if unknown, choose &quot;Other&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_website_origin_on_convert</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
LEN(TEXT( Website_origin__c ) )= 0,
 IsConverted 
)</errorConditionFormula>
        <errorDisplayField>Absolute_luxury__c</errorDisplayField>
        <errorMessage>Please ensure correct a Website Origin value is included.</errorMessage>
    </validationRules>
