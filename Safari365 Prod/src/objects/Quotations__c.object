        <fullName>Send_request_to_Wetu</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Create WETU Itinerary</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/apex.js&quot;)}
var errorFlag =  new Boolean(false);

if((&quot;{!Quotations__c.Do_not_include_Photobooks__c}&quot; == false) &amp;&amp; (&quot;{!Quotations__c.Include_Photobooks_USD__c}&quot; == false) &amp;&amp;
(&quot;{!Quotations__c.Include_Photobooks_ZAR__c}&quot; == false))
{
alert(&quot;{!$Label.Photobooks_not_included_for_wetu}&quot;);
errorFlag=true;
}

if((&quot;{!Quotations__c.Include_Passenger_Insurance_USD__c}&quot; == false) &amp;&amp; (&quot;{!Quotations__c.Include_Passenger_Insurance_ZAR__c}&quot; == false) &amp;&amp;
(&quot;{!Quotations__c.Do_not_include_insurance__c}&quot; == false)
&amp;&amp; (errorFlag== false))
{
alert(&quot;{!$Label.Passenger_Insurance_not_included_for_wetu}&quot;);
errorFlag = true;
}
if(errorFlag == false)
{
sforce.apex.execute(&quot;WetuService&quot;,&quot;processRequest&quot;,
{quotationId:&quot;{!Quotations__c.Id}&quot;,
userId:&quot;{!User.Id}&quot;,
opportunityId:&quot;{!Opportunity.Id}&quot;,
isTestRunning:false
});
location.reload();
}</url>
    </webLinks>
