    <validationRules>
        <fullName>Approval_from_FX_Treasury_before_RBC</fullName>
        <active>true</active>
        <description>IF “Approval from FX Treasury Required” = True, a consultant cannot change Quotation Stage to Raised Booking Confirmation ( or cant create Booking Confirmation doc)</description>
        <errorConditionFormula>AND(ISCHANGED(Quotation_Stage__c), ISPICKVAL(Quotation_Stage__c,&quot;Raised Invoice&quot;), Approval_from_FX_Treasury_Required__c == true,NOT($User.ByPassValidation__c))</errorConditionFormula>
        <errorMessage>Quote status cannot change stage to Raised Booking Confirmation as approval from FX Manager required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Approval_from_Finance_Required</fullName>
        <active>true</active>
        <description>A User cannot update to Confirmed unless Fiance has approved the Quotation</description>
        <errorConditionFormula>AND(ISCHANGED(Quotation_Stage__c),
Confirmed__c == TRUE,
ISPICKVAL(Quotation_Stage__c,&quot;Confirmed&quot;),
Quotation_Approval_Requested__c == false, 
NOT($User.ByPassValidation__c))</errorConditionFormula>
        <errorMessage>Quote stage cannot change to &apos;Confirmed&apos; before &apos;Fiance has approved the Quotation values&apos;. Please ensure all documents have been added and the is submitted for approval.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Status_Cancelled</fullName>
        <active>false</active>
        <errorConditionFormula>AND(NOT($User.ByPassValidation__c),ISCHANGED(Quotation_Stage__c), ISPICKVAL(Quotation_Stage__c,&quot;Cancelled&quot;), ALL_Quotation_LineItem_Count__c &lt;All_Cancelled_QuotationLineItems__c)</errorConditionFormula>
        <errorMessage>A User cannot update to Cancelled status unless all service lines are Stage:  No Booking Required, Cancelled, Not Booked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Status_Confirmed</fullName>
        <active>false</active>
        <description>A User cannot update to Confirmed unless all service lines are Stage:  Confirmed or No Booking Required.</description>
        <errorConditionFormula>AND(ISCHANGED(Quotation_Stage__c),
Confirmed__c == True,
ISPICKVAL(Quotation_Stage__c,&quot;Confirmed&quot;), All_Confirmed_QuotationLineItem__c&lt;ALL_Quotation_LineItem_Count__c,
NOT($User.ByPassValidation__c))</errorConditionFormula>
        <errorMessage>Quote stage cannot change to Confirmed unless all service lines are Stage:  Confirmed or No Booking Required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Status_ConfirmedInternalDocuments</fullName>
        <active>true</active>
        <description>A User cannot update to Confirmed unless Internal Documents Added to SF</description>
        <errorConditionFormula>AND(ISCHANGED(Quotation_Stage__c),
Confirmed__c == TRUE,
ISPICKVAL(Quotation_Stage__c,&quot;Confirmed&quot;),
Internal_Documents_have_been_Added_to_SF__c == false, 
NOT($User.ByPassValidation__c))</errorConditionFormula>
        <errorMessage>Quote stage cannot change to &apos;Confirmed&apos; before &apos;Internal Documents Added To SF&apos; has been checked. Please ensure all documents have been added and the box is checked to proceed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Status_Raised_Invoice</fullName>
        <active>true</active>
        <description>AND(Sales_Values_Verified_By_Consultant__c = TRUE,ISCHANGED(Quotation_Stage__c), ISPICKVAL(Quotation_Stage__c,&quot;Raised Invoice&quot;), 
All_RaisedInvoice_QuotationLineItem__c&lt;ALL_Quotation_LineItem_Count__c)</description>
        <errorConditionFormula>AND(Sales_Values_Verified_By_Consultant__c = FALSE,
NOT($User.ByPassValidation__c),
ISCHANGED(Quotation_Stage__c), ISPICKVAL(Quotation_Stage__c,&quot;Raised Invoice&quot;))</errorConditionFormula>
        <errorMessage>Quote stage cannot change to &apos;Raised Booking Confirmation&apos; until deposit values have been calculated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Status_Raised_Invoice_AllConfirmed</fullName>
        <active>true</active>
        <errorConditionFormula>AND (ISCHANGED(Quotation_Stage__c),
NOT($User.ByPassValidation__c),  
ISPICKVAL(Quotation_Stage__c,&quot;Raised Invoice&quot;), 
All_RaisedInvoice_QuotationLineItem__c&lt;ALL_Quotation_LineItem_Count__c)</errorConditionFormula>
        <errorMessage>The QuotationStage can only be Raise confirmation before all the Quotation line item is confirmed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quotation_Stage_Cannot_be_Changed</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED(Quotation_Stage__c), isBookingConfStatusOverdue__c == true,NOT($User.ByPassValidation__c))</errorConditionFormula>
        <errorMessage>Quote stage cannot be changed as booking confirmation status is overdue.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Book_this_is_True</fullName>
        <active>true</active>
        <description>You need to click the &quot;Book This&quot; function in the members club before you can raised a booking confirmation.  If Book this is unavailable, please contact the FX user.</description>
        <errorConditionFormula>AND(ISCHANGED(Quotation_Stage__c), ISPICKVAL(Quotation_Stage__c,&quot;Raised Invoice&quot;), Client_Has_Requested_Confirmation__c == false,NOT($User.ByPassValidation__c))</errorConditionFormula>
        <errorMessage>You need to click the &quot;Book This&quot; function in the members club before you can raise a booking confirmation.  If &quot;Book This&quot; is unavailable, please get approval from FX Manager to proceed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Confirmed_When_Funds_Received</fullName>
        <active>true</active>
        <description>User can only proceed to &apos;Confirmed&apos; when &apos;Deposit Received&apos; field is True.</description>
        <errorConditionFormula>AND(ISCHANGED(Quotation_Stage__c), ISPICKVAL(Quotation_Stage__c,&quot;Confirmed&quot;), Client_Fund_Recieved__c == false,NOT($User.ByPassValidation__c))</errorConditionFormula>
        <errorMessage>Quote stage cannot change to &apos;Confirmed&apos; before a deposit has been marked as received by the FX Manager. Please ensure your client has paid for their booking before contacting the FX Manager.</errorMessage>
    </validationRules>
