var urlSplit = document.URL.split(&quot;/&quot;);
//alert(urlSplit[3])
//alert(urlSplit.length)
if(urlSplit.length &gt;= 4){
	result = sforce.connection.query(&quot;Select Id, Name, Email__c, First_Name__c, Last_Name__c, Phone__c, Source__c, Wildlife_and_safari_experiences__c, Beach_relaxation__c, Honeymoons_and_romance__c, Walking_safaris__c, Food_and_Wine__c, Active_and_outdoor_adventure__c, History_and_culture__c, Family_safaris__c, Lead_Nurture__c, Escalation_Speed__c, Newsletter__c, Other_Interests__c, South_Africa__c, Tanzania__c, Kenya__c, Victoria_Falls__c, Botswana__c, Namibia__c, Mozambique__c, Zambia__c, Zanzibar_and_Pemba__c, Rwanda__c, Uganda__c FROM Lead_Nurture__c where Id=&apos;&quot; +urlSplit [3] + &quot;&apos;&quot;);
	records = result.getArray(&quot;records&quot;);
	//alert(records.length)
	if(records.length &gt;= 1 ){
		var leadNuture= records[0];
                var email =leadNuture[&quot;Email__c&quot;];
		//alert(email);
		result = sforce.connection.query(&quot;Select Id,Email from Lead where Email=&apos;&quot; +email + &quot;&apos;&quot;);
		//alert(&apos;lead resullt&gt;&gt;&gt;&apos;+result);
		records = result.getArray(&quot;records&quot;);
