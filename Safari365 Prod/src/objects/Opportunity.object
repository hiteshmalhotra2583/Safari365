    <validationRules>
        <fullName>Arrival_Date_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT($User.ByPassValidation__c),ISPICKVAL( StageName , &apos;Closed Won Deal&apos;), ISBLANK( Arrival_Date__c ))</errorConditionFormula>
        <errorDisplayField>Arrival_Date__c</errorDisplayField>
        <errorMessage>Travel Dates are required if this is a confirmed traveling client.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Change_Currency_On_Client_Currency_Locke</fullName>
        <active>true</active>
        <description>The client currency can only be changed in Sales Force if the &apos;Client Currency Locked&apos; field is = False.</description>
        <errorConditionFormula>AND(ISCHANGED(Client_Display_Currency__c),Client_Currency_Locked__c == true,NOT($User.ByPassValidation__c))</errorConditionFormula>
        <errorMessage>The Client Currency cannot be changed as it is locked, please get approval from FX Manager.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Lost_Reason_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL( StageName  , &quot;Closed Lost&quot;),
OR( 
ISPICKVAL( Closed_Lost_Reason__c , &quot;&quot;)))</errorConditionFormula>
        <errorMessage>Please insert a closed lost reason ( and additional internal comments if possible)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Customer_Service_Support_Description</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT($User.ByPassValidation__c),
ISPICKVAL(Service_Level_Severity__c ,&quot;Low (Easily resolved issue, no need for followup)&quot;),
OR( 
ISBLANK(Support_Aservice_Description__c)))</errorConditionFormula>
        <errorDisplayField>Service_Level_Severity__c</errorDisplayField>
        <errorMessage>Please detail the nature of the incident</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Customer_Service_Support_Description_2</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT($User.ByPassValidation__c),
ISPICKVAL(Service_Level_Severity__c ,&quot;Medium (Client agitated and marginally upset, Consultant to followup with client, Team Leader to be aware)&quot;),
OR( 
ISBLANK(Support_Aservice_Description__c)))</errorConditionFormula>
        <errorMessage>Error To be Written</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GP_Required</fullName>
        <active>true</active>
        <description>Need GP figures entered before MITB</description>
        <errorConditionFormula>AND( NOT($User.ByPassValidation__c),ISPICKVAL( StageName , &apos;Closed Won Deal&apos;), ISBLANK(Gross_Profit__c))</errorConditionFormula>
        <errorDisplayField>Gross_Profit__c</errorDisplayField>
        <errorMessage>Need GP figures entered before MITB</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Client_Currency</fullName>
        <active>true</active>
        <description>The Client Currency cannot be changed as this has been locked, please get approval from FX Manager.</description>
        <errorConditionFormula>AND(
NOT($User.ByPassValidation__c),
 Client_Currency_Locked__c == TRUE,
ISCHANGED(Client_Display_Currency__c),
OR(
ISPICKVAL(PRIORVALUE(Client_Display_Currency__c) , &quot;AUD&quot;),
ISPICKVAL(PRIORVALUE(Client_Display_Currency__c) , &quot;GBP&quot;),
ISPICKVAL(PRIORVALUE(Client_Display_Currency__c) , &quot;USD&quot;),
ISPICKVAL(PRIORVALUE(Client_Display_Currency__c) , &quot;EUR&quot;)
))</errorConditionFormula>
        <errorMessage>The Client Currency cannot be changed as this has been locked, please get approval from FX Manager.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Stage_ClosedWonDeal_Confirm</fullName>
        <active>true</active>
        <description>The opportunity Stage &quot;Closed Won Deal&quot; can only set when one of the Quotation is confirmed</description>
        <errorConditionFormula>AND( 
NOT($User.ByPassValidation__c), 
ISCHANGED(StageName), 
ISPICKVAL(StageName,&quot;Closed Won Deal&quot;), 
Confirmed_Quote_is_in_Travelogic__c = False, 
Confirmed_Quotation_Count__c &lt;1 

)</errorConditionFormula>
        <errorMessage>The opportunity Stage &quot;Closed Won Deal&quot; can only set when one of the Quotation is confirmed or the field &quot;Confirmed Quote in TL&quot; is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Stage_MIB</fullName>
        <active>false</active>
        <description>The opportunity Stage MIB can only set when one of the Quotation is confirmed</description>
        <errorConditionFormula>AND(NOT($User.ByPassValidation__c),ISCHANGED(StageName), ISPICKVAL(StageName,&quot;Money in the Bank&quot;), Confirmed_Quotation_Count__c &lt;1 )</errorConditionFormula>
        <errorMessage>The Opportunity Stage &quot; Money in the bank&quot; can only set when one of the Quotation  is confirmed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PhotoBook_Sent_stage_Tracking_Number</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT($User.ByPassValidation__c),
ISPICKVAL(Photobook_Stage__c  , &quot;Sent&quot;),
OR( 
ISBLANK(PhotoBook_Tracking_Number__c)))</errorConditionFormula>
        <errorDisplayField>PhotoBook_Tracking_Number__c</errorDisplayField>
        <errorMessage>Please insert Tracking Number before you change the photo book stage to &apos;sent&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Referral_Order_Tracking_Details_Included</fullName>
        <active>true</active>
        <description>&apos;Tracking number&apos; and &apos;which gift was sent&apos; must be included</description>
        <errorConditionFormula>AND(NOT($User.ByPassValidation__c),
ISPICKVAL(Gift_Sent_to_this_Opportunity_Referrer__c , &quot;Gift Sent&quot;),
OR(
ISBLANK(Gift_Tracking_Order_Details__c),
OR(
ISBLANK( Which_Gift_was_Sent__c ))))</errorConditionFormula>
        <errorDisplayField>Gift_Tracking_Order_Details__c</errorDisplayField>
        <errorMessage>Tracking number or delivery details must be included before confirming</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sale_Required</fullName>
        <active>true</active>
        <description>Need Sale figures entered before MITB</description>
        <errorConditionFormula>AND( NOT($User.ByPassValidation__c),ISPICKVAL( StageName , &apos;Closed Won Deal&apos;), ISBLANK(  Amount))</errorConditionFormula>
        <errorDisplayField>Amount</errorDisplayField>
        <errorMessage>Need Sale figures entered before MITB</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Service_Request_Comments_Require</fullName>
        <active>false</active>
        <description>Add Date and Comments to the Support Service Description</description>
        <errorConditionFormula>AND(
NOT(ISPICKVAL( Service_Level_Severity__c , &quot; ISBLANK(expression)&quot;)),(
ISBLANK(Support_Aservice_Description__c)))</errorConditionFormula>
        <errorDisplayField>Support_Aservice_Description__c</errorDisplayField>
        <errorMessage>Add Date and Comments to the Support Service Description</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TravelEndDateShouldBeGreaterThanStartDat</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT($User.ByPassValidation__c),TravelEndDate__c &lt;  Arrival_Date__c )</errorConditionFormula>
        <errorDisplayField>TravelEndDate__c</errorDisplayField>
        <errorMessage>Travel End Date should be greater than Travel Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TravelStartDateShouldBeLessThanEndDate</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT($User.ByPassValidation__c),Arrival_Date__c &gt;   TravelEndDate__c  )</errorConditionFormula>
        <errorDisplayField>Arrival_Date__c</errorDisplayField>
        <errorMessage>Travel Start Date should be before Travel End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>cos_Required</fullName>
        <active>true</active>
        <description>Need COST figures entered before MITB</description>
        <errorConditionFormula>AND( NOT($User.ByPassValidation__c),ISPICKVAL( StageName , &apos;Closed Won Deal&apos;), ISBLANK(  CoS__c))</errorConditionFormula>
        <errorDisplayField>CoS__c</errorDisplayField>
        <errorMessage>Need COST figures entered before MITB</errorMessage>
    </validationRules>
